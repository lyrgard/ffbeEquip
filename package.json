    "@sindresorhus/is": "0.11.0",
    "bluebird": "3.5.1",
    "body-parser": "^1.18.3",
    "boom": "7.2.0",
    "client-sessions": "0.8.0",
    "crypto-random-string": "1.0.0",
    "express": "^4.16.3",
    "firebase-admin": "^5.12.0",
    "googleapis": "26.0.1",
    "helmet": "^3.13.0",
    "inquirer": "6.2.0",
    "joi": "13.6.0",
